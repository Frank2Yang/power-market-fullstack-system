{
  "name": "power-market-system",
  "version": "1.0.0",
  "description": "电力市场预测与投标优化系统 - 基于2025年真实数据的全栈应用",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "next dev -p 3001",
    "build": "next build",
    "start": "node server.js",
    "build-client": "next build && next export",
    "vercel-build": "npm run build-client"
  },
  "keywords": [
    "电力市场",
    "预测分析", 
    "投标优化",
    "真实数据",
    "全栈应用"
  ],
  "author": "Frank Yang",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "xlsx": "^0.18.5",
    "mathjs": "^11.11.0",
    "ml-regression": "^6.0.1",
    "next": "14.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "axios": "1.6.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
